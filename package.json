{
  "name": "ogeobubu",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "start": "node server.js",
    "serve": "npm run build && npm run start",
    "analyze": "npm run build && open dist/stats.html",
    "optimize-images": "node scripts/optimize-images.js",
    "optimize-css": "node scripts/optimize-css.js",
    "monitor-performance": "node scripts/performance-monitor.js",
    "boost-performance": "node scripts/boost-performance.js",
    "build:optimized": "npm run optimize-images && npm run optimize-css && npm run build",
    "build:full": "npm run build:optimized && npm run monitor-performance",
    "build:boosted": "npm run boost-performance && npm run build:optimized",
    "test": "vitest"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.7",
    "@tailwindcss/vite": "^4.1.11",
    "aos": "2.3.4",
    "compression": "^1.8.1",
    "express": "^5.1.0",
    "framer-motion": "^10.18.0",
    "gatsby-plugin-mdx": "^5.14.1",
    "lucide-react": "^0.536.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.1",
    "react-scroll": "^1.8.9"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/aos": "^3.0.4",
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@types/react-scroll": "^1.8.5",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.21",
    "cssnano": "^7.1.0",
    "postcss": "^8.5.6",
    "rollup-plugin-visualizer": "^6.0.3",
    "sharp": "^0.34.3",
    "tailwindcss": "^4.1.11",
    "terser": "^5.43.1",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vite-plugin-compression2": "^2.2.0",
    "vitest": "^1.3.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
